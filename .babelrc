{
    "presets": [
        "@babel/preset-env",
        "@babel/preset-react"
    ],
    "plugins": [
        "@babel/plugin-proposal-class-properties",
        ["babel-plugin-css-modules-transform", {
            "preprocessCss": "./cssPreprocess.js",
            "extensions": [".scss", ".sass"]
        }],
        ["@babel/plugin-transform-runtime",
            {
              "regenerator": true
            }
        ]
    ]
}
